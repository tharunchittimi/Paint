<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="@dimen/dp37"
        android:layout_height="@dimen/dp27"
        android:foregroundGravity="center"
        android:src="@drawable/ic_person1"
        android:tint="@color/icon_grey"
        app:layout_constraintBottom_toTopOf="@id/textView"
        app:layout_constraintStart_toStartOf="@id/guideline3"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txttt"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="User Details"
        android:textColor="@color/Black"
        android:textSize="@dimen/sp18"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@id/imageView1"
        app:layout_constraintTop_toTopOf="@id/imageView1"
        app:layout_constraintVertical_bias="0.0" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.02" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="@dimen/dp37"
        android:layout_height="@dimen/dp27"
        android:layout_marginTop="@dimen/dp20"
        android:src="@drawable/ic_next"
        android:tint="@color/light_grey"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp1"
        android:layout_marginLeft="@dimen/dp15"
        android:layout_marginTop="@dimen/dp14"
        android:background="@color/light_grey"
        android:tint="@android:color/tertiary_text_light"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@id/guideline3"
        app:layout_constraintTop_toBottomOf="@id/imageView2" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="@dimen/dp37"
        android:layout_height="@dimen/dp27"
        android:layout_marginTop="@dimen/dp15"
        android:src="@drawable/ic_order"
        android:tint="@color/icon_grey"
        app:layout_constraintStart_toStartOf="@id/guideline3"
        app:layout_constraintTop_toBottomOf="@id/textView" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Order Status"
        android:textColor="@color/Black"
        android:textSize="@dimen/sp18"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/imageView3"
        app:layout_constraintTop_toTopOf="@id/imageView3" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="@dimen/dp37"
        android:layout_height="@dimen/dp27"
        android:layout_marginTop="@dimen/dp20"
        android:src="@drawable/ic_next"
        android:tint="@color/light_grey"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/textView" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp1"
        android:layout_marginLeft="@dimen/dp15"
        android:layout_marginTop="@dimen/dp14"
        android:background="@color/light_grey"
        android:tint="@android:color/tertiary_text_light"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@id/guideline3"
        app:layout_constraintTop_toBottomOf="@id/imageView3" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="@dimen/dp37"
        android:layout_height="@dimen/dp27"
        android:layout_marginTop="@dimen/dp15"
        android:src="@drawable/ic_fav1"
        android:tint="@color/icon_grey"
        app:layout_constraintStart_toStartOf="@id/guideline3"
        app:layout_constraintTop_toBottomOf="@id/textView1" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Wishlist"
        android:textColor="@color/Black"
        android:textSize="@dimen/sp18"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/imageView5"
        app:layout_constraintTop_toTopOf="@id/imageView5" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="@dimen/dp37"
        android:layout_height="@dimen/dp27"
        android:layout_marginTop="@dimen/dp20"
        android:src="@drawable/ic_next"
        android:tint="@color/light_grey"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/textView1" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp1"
        android:layout_marginLeft="@dimen/dp15"
        android:layout_marginTop="@dimen/dp14"
        android:background="@color/light_grey"
        android:tint="@android:color/tertiary_text_light"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@id/guideline3"
        app:layout_constraintTop_toBottomOf="@id/imageView5" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="@dimen/dp37"
        android:layout_height="@dimen/dp27"
        android:layout_marginTop="@dimen/dp15"
        android:src="@drawable/ic_faq"
        android:tint="@color/icon_grey"
        app:layout_constraintStart_toStartOf="@id/guideline3"
        app:layout_constraintTop_toBottomOf="@id/textView2" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="FAQ's"
        android:textColor="@color/Black"
        android:textSize="@dimen/sp18"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/imageView7"
        app:layout_constraintTop_toTopOf="@id/imageView7" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="@dimen/dp37"
        android:layout_height="@dimen/dp27"
        android:layout_marginTop="@dimen/dp20"
        android:src="@drawable/ic_next"
        android:tint="@color/light_grey"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/textView2" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp1"
        android:layout_marginLeft="@dimen/dp15"
        android:layout_marginTop="@dimen/dp14"
        android:background="@color/light_grey"
        android:tint="@android:color/tertiary_text_light"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@id/guideline3"
        app:layout_constraintTop_toBottomOf="@id/imageView7" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="@dimen/dp37"
        android:layout_height="@dimen/dp27"
        android:layout_marginTop="@dimen/dp15"
        android:src="@drawable/ic_blog"
        android:tint="@color/icon_grey"
        app:layout_constraintStart_toStartOf="@id/guideline3"
        app:layout_constraintTop_toBottomOf="@id/textView3" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Blog"
        android:textColor="@color/Black"
        android:textSize="@dimen/sp18"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/imageView9"
        app:layout_constraintTop_toTopOf="@id/imageView9" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="@dimen/dp37"
        android:layout_height="@dimen/dp27"
        android:layout_marginTop="@dimen/dp20"
        android:src="@drawable/ic_next"
        android:tint="@color/light_grey"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/textView3" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp1"
        android:layout_marginLeft="@dimen/dp15"
        android:layout_marginTop="@dimen/dp14"
        android:background="@color/light_grey"
        android:tint="@android:color/tertiary_text_light"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@id/guideline3"
        app:layout_constraintTop_toBottomOf="@id/imageView9" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="@dimen/dp37"
        android:layout_height="@dimen/dp27"
        android:layout_marginTop="@dimen/dp15"
        android:src="@drawable/ic_chat"
        android:tint="@color/icon_grey"
        app:layout_constraintStart_toStartOf="@id/guideline3"
        app:layout_constraintTop_toBottomOf="@id/textView4" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Chat With Us"
        android:textColor="@color/Black"
        android:textSize="@dimen/sp18"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/imageView11"
        app:layout_constraintTop_toTopOf="@id/imageView11" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="@dimen/dp37"
        android:layout_height="@dimen/dp27"
        android:layout_marginTop="@dimen/dp20"
        android:src="@drawable/ic_next"
        android:tint="@color/light_grey"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/textView4" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp1"
        android:layout_marginLeft="@dimen/dp15"
        android:layout_marginTop="@dimen/dp14"
        android:background="@color/light_grey"
        android:tint="@android:color/tertiary_text_light"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@id/guideline3"
        app:layout_constraintTop_toBottomOf="@id/imageView11" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="@dimen/dp37"
        android:layout_height="@dimen/dp27"
        android:layout_marginTop="@dimen/dp15"
        android:src="@drawable/ic_signout"
        android:tint="@color/icon_grey"
        app:layout_constraintStart_toStartOf="@id/guideline3"
        app:layout_constraintTop_toBottomOf="@id/textView5" />

    <TextView
        android:id="@+id/textText"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Signout"
        android:textColor="@color/Black"
        android:textSize="@dimen/sp18"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/imageView13"
        app:layout_constraintTop_toTopOf="@id/imageView13" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp1"
        android:layout_marginLeft="@dimen/dp15"
        android:layout_marginTop="@dimen/dp14"
        android:background="@color/light_grey"
        android:tint="@android:color/tertiary_text_light"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@id/guideline3"
        app:layout_constraintTop_toBottomOf="@id/imageView13" />

</androidx.constraintlayout.widget.ConstraintLayout>